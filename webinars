        }
        
        /* Program sections */
        .program-section {
            margin: 3rem 0;
        }
        
        h2 {
            color: var(--primary);
            border-bottom: 2px solid var(--secondary);
            padding-bottom: 0.5rem;
            margin-bottom: 1.5rem;
        }
        
