<header>
    <h1>AccessWorld Seminars Schedule: Jan – Jun 2026</h1>
    <p>All sessions are Thursday evenings & Friday mornings. Major 2-day seminars highlighted. Times will display in your local timezone.</p>
    <label for="timezone">Select your time zone:</label>
    <select id="timezone">
        <option value="">Auto-detect</option>
        <option value="Australia/Sydney">Sydney (AEST)</option>
        <option value="America/New_York">New York (EST)</option>
        <option value="Europe/London">London (GMT)</option>
        <option value="Asia/Kolkata">India (IST)</option>
        <option value="America/Los_Angeles">Los Angeles (PST)</option>
    </select>
</header>

<script>
    function updateTimes(tz = Intl.DateTimeFormat().resolvedOptions().timeZone) {
        document.querySelectorAll('li[data-utc]').forEach(function(li){
            const utcTime = li.getAttribute('data-utc');
            const localTime = new Date(utcTime);
            li.querySelector('.local-time').textContent = localTime.toLocaleString('en-US', { 
                weekday: 'short', year: 'numeric', month: 'short', day: 'numeric',
                hour: '2-digit', minute: '2-digit', hour12: true, timeZone: tz
            });
        });
    }

    const select = document.getElementById('timezone');
    select.addEventListener('change', () => {
        const tz = select.value || Intl.DateTimeFormat().resolvedOptions().timeZone;
        updateTimes(tz);
    });

    // Initial load
    updateTimes();
</script>

